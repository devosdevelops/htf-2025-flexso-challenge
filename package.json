{
  "name": "htf2025",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/cds-types": "^0.6.5",
    "@sap/cds": "^8.9.7",
    "express": "^4",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6.5",
    "@cap-js/sqlite": "^1",
    "cds-plugin-ui5": "^0.9.3"
  },
  "scripts": {
    "start": "cds-ts watch --livereload false",
    "build-apps": "npm-run-all --parallel watch-build-baserepair watch-build-securityoverview watch-build-sonaroverview watch-build-symboltranslation",
    "watch-build-baserepair": "cd app/baserepair && ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "watch-build-securityoverview": "cd app/securityoverview && ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "watch-build-sonaroverview": "cd app/sonaroverview && ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "watch-build-symboltranslation": "cd app/symboltranslation && ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "watch-symboltranslation": "cds watch --open flexso.cap.htf.symboltranslation/index.html?sap-ui-xx-viewCache=false --livereload true",
    "watch-baserepair": "cds watch --open flexso.cap.htf.baserepair/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-securityoverview": "cds watch --open flexso.cap.htf.securityoverview/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-sonaroverview": "cds watch --open flexso.cap.hrf.sonaroverview/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "workspaces": [
    "app/*"
  ]
}
